#include <stdio.h>

int main() {
    int X[10];
    int i;

    // lÃª os valores do vetor X
    for(i = 0; i < 10; i++) {
        scanf("%d", &X[i]);
    }

    // substitui os valores negativos e nulos por 1
    for(i = 0; i < 10; i++) {
        if(X[i] <= 0) {
            X[i] = 1;
        }
    }

    // exibe o vetor X modificado
    for(i = 0; i < 10; i++) {
        printf("X[%d] = %d\n", i, X[i]);
    }

    return 0;
}
